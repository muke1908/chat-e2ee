{
  "compilerOptions": {
    "outDir": "dist/types",
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "declaration": true,
    "strictNullChecks": false,
    "downlevelIteration": true,
    "lib": [
      "ESNext",
      "dom"
    ],
  }
}